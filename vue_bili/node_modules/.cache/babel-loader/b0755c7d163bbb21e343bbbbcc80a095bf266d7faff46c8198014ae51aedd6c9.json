      <a-form-item>\n              <a-button :disabled=\"disabled\" type=\"primary\" html-type=\"submit\" class=\"login-form-button\">\n                Log in\n              </a-button>\n              Or\n              <a href=\"\">register now!</a>\n            </a-form-item>\n          </a-form>\n        </template>\n      </a-layout-content>\n      <a-layout-footer id=\"layout_foot\" :style=\"{ textAlign: 'center' }\">\n        Ant Design Â©2018 Created by Ant UED\n      </a-layout-footer>\n    </a-layout>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n      remember: true,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.login_body_form {\n  background-color: black;\n  width: 500px;\n  height: 200px;\n  position: fixed;\n}\n#layout-outter {\n  min-height: 100vh;\n}\n\n#layout_head {\n  background-color: rgba(255, 255, 255, 0.2);\n  height: 150px;\n}\n\n#layout_body {\n  background-color: rgba(255, 255, 255, 0.2);\n}\n\n#layout_foot {\n  background-color: rgba(255, 255, 255, 0.2);\n}\n\n#form_template {\n  left: 0%;\n}\n</style>\n"],"mappings":"AAiEA;EACAA,KAAA;IACA;MACAC,QAAA;MACAC,QAAA;MACAC,QAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}