"{\n            background: '#fff',\n            padding: '24px',\n            height: '500px',\n            margin: '20px 5px',\n            overflow: 'initial',\n            top: '64px',\n          }\"\n        >\n          <div\n            style=\"\n              display: flex;\n              flex-direction: column;\n              align-items: center;\n              text-align: center;\n            \"\n          >\n            <div>\n              <div style=\"margin-bottom: 16px\">\n                showLine: <a-switch v-model=\"showLine\" />\n                <br />\n                <br />\n                showIcon: <a-switch v-model=\"showIcon\" />\n              </div>\n              <a-tree\n                :show-line=\"showLine\"\n                :show-icon=\"showIcon\"\n                :default-expanded-keys=\"['0-0-0', '0-0-1', '0-0-2']\"\n                @select=\"onSelect\"\n              >\n                <a-icon slot=\"icon\" type=\"carry-out\" />\n                <a-tree-node key=\"0-0\">\n                  <a-icon slot=\"icon\" type=\"carry-out\" />\n                  <span slot=\"title\" style=\"color: #1890ff\">parent 1</span>\n                  <a-tree-node key=\"0-0-0\" title=\"parent 1-0\">\n                    <a-icon slot=\"icon\" type=\"carry-out\" />\n                    <a-tree-node key=\"0-0-0-0\" title=\"leaf\">\n                      <a-icon slot=\"icon\" type=\"carry-out\" />\n                    </a-tree-node>\n                    <a-tree-node key=\"0-0-0-1\" title=\"leaf\">\n                      <a-icon slot=\"icon\" type=\"carry-out\" />\n                    </a-tree-node>\n                    <a-tree-node key=\"0-0-0-2\" title=\"leaf\">\n                      <a-icon slot=\"icon\" type=\"carry-out\" />\n                    </a-tree-node>\n                  </a-tree-node>\n                  <a-tree-node key=\"0-0-1\" title=\"parent 1-1\">\n                    <a-icon slot=\"icon\" type=\"carry-out\" />\n                    <a-tree-node key=\"0-0-1-0\" title=\"leaf\">\n                      <a-icon slot=\"icon\" type=\"carry-out\" />\n                    </a-tree-node>\n                  </a-tree-node>\n                  <a-tree-node key=\"0-0-2\" title=\"parent 1-2\">\n                    <a-icon slot=\"icon\" type=\"carry-out\" />\n                    <a-tree-node key=\"0-0-2-0\" title=\"leaf\">\n                      <a-icon slot=\"icon\" type=\"carry-out\" />\n                    </a-tree-node>\n                    <a-tree-node key=\"0-0-2-1\" title=\"leaf\">\n                      <a-icon slot=\"icon\" type=\"carry-out\" />\n                      <a-icon slot=\"switcherIcon\" type=\"form\" />\n                    </a-tree-node>\n                  </a-tree-node>\n                </a-tree-node>\n              </a-tree>\n            </div>\n          </div>\n        </a-layout-content>\n      </a-col>\n      <a-col :span=\"18\">\n        <a-layout-content\n          :style=\"{\n            background: '#fff',\n            padding: '24px',\n            height: '500px',\n            overflow: 'initial',\n            margin: '20px 5px',\n            top: '64px',\n          }\"\n        >\n          <span>\n            <a-input-search style=\"width: 200px\" @search=\"onSearch\" />\n            <a-button type=\"primary\" style=\"margin-left: 10%\">Primary</a-button>\n          </span>\n          <br />\n          <br />\n          <a-table\n            :columns=\"columns\"\n            :data-source=\"positionList\"\n            :scroll=\"{ x: 1500, y: 300 }\"\n            :v-for=\"(position, index) in positionList\"\n            @change=\"onChange\"\n          >\n            <a slot=\"action\">action</a>\n          </a-table>\n          <span>{{ positionList }}</span>\n        </a-layout-content>\n      </a-col>\n    </a-row>\n  </div>\n</template>\n<script>\nconst columns = [\n  {\n    title: \"序号\",\n    dataIndex: \"positionNumber\",\n    key: \"positionNumber\",\n    width: 150,\n  },\n  {\n 